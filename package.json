{
  "name": "wsevents",
  "version": "1.0.0",
  "scripts": {
    "precommit": "lint-staged",
    "js-fix": "eslint --fix src",
    "css-fix": "prettier --write \"src/**/*.css\"",
    "fix": "npm run js-fix && npm run css-fix",
    "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build": "rm -rf ./build && NODE_ENV=production webpack",
    "publish": "rsync -avh --chmod=ugo=rwX build/ lonesadida@lonesadida.ru:domains/wsevents.ru/public_html"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "caniuse-db": "^1.0.30000709",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-loader": "^1.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.1.1",
    "prettier": "^1.5.3",
    "prettier-eslint-cli": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-autobind": "^1.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "core-js": "^2.5.1",
    "moment": "^2.18.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
